{
  "name": "OpenStreetMap",
  "description" : "Minimalistic New tab page that displays interesting places on map.",
  "version": "0.1",
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png",
    "48": "icon_48.png"
  },
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "permissions": [
    "bookmarks",
	"http://www.openstreetmap.org/",
	"http://cdn.leafletjs.com"
  ],
  "manifest_version": 1
}
